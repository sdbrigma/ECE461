#include "msp432.h"
#include "stdint.h"
#include "driverlib.h"
#include "functions.h"
#include "macros.h"


void port5IsrHandler(){
	doorbell();
}

void port3IsrHandler(){
	burglar_alarm();
}

uint32_t adcResult;
void adc14IsrHandler(){
	if(ADC14_getResult(ADC_MEM10) >= BURGLAR){
		adcResult = 14;
	}
	else{
		adcResult = 20;
	}
	ADC14_clearInterruptFlag(ADC_INT10);
}
