void doorbell(){

	TA0CTL = TIMER_A_UP_MODE;
	Timer_A_PWMConfig pwmConfig =
	{
	        TIMER_A_CLOCKSOURCE_SMCLK,
	        TIMER_A_CLOCKSOURCE_DIVIDER_1,
	        PWM_PERIOD,
	        TIMER_A_CAPTURECOMPARE_REGISTER_4,
	        TIMER_A_OUTPUTMODE_RESET_SET,
	        DING1,
	};
}


