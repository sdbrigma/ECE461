#include "msp432.h"
#include "stdint.h"
#include "functions.h"
#include "macros.h"
#include "driverlib.h"

void Init_Eusci(){
	UCSWRST = 1;// enable changes to control register
	// configure in master mode using I2C
	UCB1CTLW0 = BITB | BITA | BIT9 | BIT7 | BIT6 | BIT4;

	UCSWRST = 0;
}
